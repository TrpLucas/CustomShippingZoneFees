<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">


    <route id="custom.shipping.zone.fees.create" path="/admin/module/CustomShippingZoneFees/create">
        <default key="_controller">CustomShippingZoneFees\Controller\CustomShippingZoneFeesController::createShippingZoneAction</default>
    </route>

    <route id="custom.shipping.zone.fees.delete" path="/admin/module/CustomShippingZoneFees/delete/{id}">
        <default key="_controller">CustomShippingZoneFees\Controller\CustomShippingZoneFeesController::deleteShippingZoneAction</default>
    </route>

    <route id="custom.shipping.zone.fees.update" path="/admin/module/CustomShippingZoneFees/update/{id}">
        <default key="_controller">CustomShippingZoneFees\Controller\CustomShippingZoneFeesController::updateShippingZoneAction</default>
    </route>

    <route id="custom.shipping.zone.fees.edit.page" path="/admin/module/CustomShippingZoneFees/edit/{id}">
        <default key="_controller">CustomShippingZoneFees\Controller\CustomShippingZoneFeesController::renderShippingZonePageAction</default>
    </route>

    <route id="custom.shipping.zone.fees.module.update" path="/admin/module/CustomShippingZoneFees/updateModule/{moduleId}">
        <default key="_controller">CustomShippingZoneFees\Controller\DeliveryModuleController::updateDeliveryModuleShippingZoneAction</default>
    </route>

    <route id="custom.shipping.zone.fees.module.remove" path="/admin/module/CustomShippingZoneFees/remove/{id}">
        <default key="_controller">CustomShippingZoneFees\Controller\DeliveryModuleController::removeDeliveryModuleShippingZoneAction</default>
    </route>

</routes>
